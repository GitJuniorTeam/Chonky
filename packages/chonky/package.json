{
  "name": "chonky",
  "version": "2.3.2",
  "description": "A File Browser component for React",
  "license": "MIT",
  "main": "./dist/chonky.cjs",
  "module": "./dist/chonky.mjs",
  "types": "./dist/chonky.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/chonky.d.mts",
        "default": "./dist/chonky.mjs"
      },
      "require": {
        "types": "./dist/chonky.d.cts",
        "default": "./dist/chonky.cjs"
      }
    },
    "./min": {
      "import": {
        "types": "./dist/chonky.d.mts",
        "default": "./dist/chonky.min.mjs"
      },
      "require": {
        "types": "./dist/chonky.d.cts",
        "default": "./dist/chonky.min.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm dist/* && rollup --config rollup.config.mjs",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "homepage": "https://chonky.io/",
  "author": {
    "name": "Timur Kuzhagaliyev",
    "email": "tim.kuzh@gmail.com",
    "url": "https://foxypanda.me/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimboKZ/Chonky.git"
  },
  "bugs": {
    "url": "https://github.com/TimboKZ/Chonky/issues"
  },
  "keywords": [
    "file",
    "chonky",
    "file-dialog",
    "file-browser",
    "react-component"
  ],
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@reduxjs/toolkit": "^1.9.7",
    "classnames": "^2.3.2",
    "deepmerge": "^4.3.1",
    "exact-trie": "^1.0.13",
    "fast-sort": "^3.4.0",
    "filesize": "^10.1.0",
    "fuzzy-search": "^3.2.1",
    "hotkeys-js": "^3.12.0",
    "nanoid": "^5.0.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-intl": "^5.25.1",
    "react-jss": "^10.10.0",
    "react-redux": "^8.1.3",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "redux-watch": "^1.2.0",
    "tsdef": "^0.0.14"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@testing-library/react": "^14.0.0",
    "@types/fuzzy-search": "^2.1.2",
    "@types/react": "^18.2.25",
    "@types/react-dom": "^18.2.11",
    "@types/react-window": "^1.8.6",
    "@types/redux-watch": "^1.1.0",
    "@types/shortid": "^0.0.30",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "publint": "^0.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.0.2",
    "rollup-plugin-dts": "^6.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
